comment on column MNT_SUBJECT_RESOURCE_GRAPH.XTITLE
  is 'X轴标题';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.YTITLE
  is 'Y轴标题';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.ISDISPLAYLEGEND
  is '是否显示图例';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.DSCODE
  is '所属数据源ID';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.SERIALNO
  is '显示序号';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.ISDEFAULT
  is '是否默认显示';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.UPDATEPERSON
  is '更新人';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.UPDATEDATE
  is '更新日期';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.REMARK
  is '备注';
alter table MNT_SUBJECT_RESOURCE_GRAPH
