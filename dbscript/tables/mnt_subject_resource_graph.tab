  RSCODE          VARCHAR2(20) not null,
  BRANCHDESC      VARCHAR2(10),
  GRAPHTYPE       VARCHAR2(10) not null,
  TITLE           VARCHAR2(10),
  XTITLE          VARCHAR2(10),
  YTITLE          VARCHAR2(10),
  ISDISPLAYLEGEND VARCHAR2(10),
  DSCODE          VARCHAR2(10),
  SERIALNO        VARCHAR2(10),
  ISDEFAULT       VARCHAR2(10),
  UPDATEPERSON    VARCHAR2(10),
  UPDATEDATE      VARCHAR2(10),
  REMARK          VARCHAR2(10)
)
;
comment on column MNT_SUBJECT_RESOURCE_GRAPH.BRANCHID
  is '主题子项ID';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.RSCODE
  is '主题ID';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.BRANCHDESC
  is '主题项描述';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.GRAPHTYPE
  is '主题类型';
comment on column MNT_SUBJECT_RESOURCE_GRAPH.TITLE
