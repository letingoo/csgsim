package sourceCode.ocableroute.model
{

	[Bindable]
	[RemoteClass(alias="ocableroute.model.OcableFiber")]
	public class OcableFiber
	{
		/** 光纤编码*/
		private var fibercode:String;

		/** 光纤名称*/
		private var fibername:String;

		/** 所属光缆段编码 */
		private var ocablecode:String;

		/** 所属光缆段名称*/
		private var ocablename:String;

		/** 光路路由中显示名称 */
		private var fibershow:String;

		/** 光纤序号*/
		private var fiberserial:String;

		public function OcableFiber()
		{
			this.fibercode="";
			this.fibername="";
			this.ocablecode="";
			this.ocablename="";
			this.fiberserial="";
			this.fibershow="";
		}
	}
}