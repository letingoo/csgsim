package sourceCode.sysGraph.model
{
	public class Location
	{
		private var _systemcode:String;
		private var _systemname:String;
		private var _x:Number;
		private var _y:Number;
		private var _used:Boolean;
		private var _expand:Boolean;
		
		public function Location(_systemcode:String,_systemname:String,_x:Number,_y:Number,_expand:Boolean=false)
		{
			this._systemcode=_systemcode;
			this._systemname=_systemname;
			this._x=_x;
			this._y=_y;
			this._expand=_expand;
		}
		
		public function get systemname():String
		{
			return _systemname;
		}

		public function set systemname(value:String):void
		{
			_systemname = value;
		}

		public function get expand():Boolean
		{
			return _expand;
		}

		public function set expand(value:Boolean):void
		{
			_expand = value;
		}

		public function get used():Boolean
		{
			return _used;
		}

		public function set used(value:Boolean):void
		{
			_used = value;
		}

		public function get systemcode():String
		{
			return _systemcode;
		}

		public function set systemcode(value:String):void
		{
			_systemcode = value;
		}

		public function get y():Number
		{
			return _y;
		}

		public function set y(value:Number):void
		{
			_y = value;
		}

		public function get x():Number
		{
			return _x;
		}

		public function set x(value:Number):void
		{
			_x = value;
		}

	}
}