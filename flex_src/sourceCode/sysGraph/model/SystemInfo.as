package sourceCode.sysGraph.model
{
	[Bindable]	
	[RemoteClass(alias="fiberwire.model.SystemInfoModel")] 
	public class SystemInfo
	{
		public var systemcode:String;
		public var systemname:String;
		public var x_capacity:String;
		public var projectname:String;
		public var tranmodel:String;
		public var vendor:String;
		public var remark:String;
		public var x:String;
		public var y:String;		
			
		public function SystemInfo()
		{
			this.systemcode = "";
			this.systemname = "";
			this.x_capacity="";
			this.projectname="";
			this.tranmodel="";
			this.vendor="";
			this.remark="";
			this.x="0";
			this.y="0";
		}
		
		public function clear():void
		{
			this.systemcode = "";
			this.systemname = "";
			this.x_capacity="";
			this.projectname="";
			this.tranmodel="";
			this.vendor="";
			this.remark="";
			this.x="0";
			this.y="0";
		}
	}
}