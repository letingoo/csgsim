package sourceCode.sysGraph.actionscript
{
	public class ActionTile
	{
		private var _imageName:String;
		private var _label:String;
		private var _shapeType:String;
		//Follower
		private var _width:Number;
		private var _height:Number;
		private var _card_image:String;
		private var _iconName:String;
		private var _labelxoff:Number;
		private var _labelyoff:Number;
		//Grid
		private var _row_count:Number;
		private var _col_count:Number;
		private var _border:Number;
		private var _border_top:Number;
		private var _padding:Number;
		private var _grid_deep:Number;
		private var _cell_deep:Number;

		public function get grid_deep():Number
		{
			return _grid_deep;
		}

		public function set grid_deep(value:Number):void
		{
			_grid_deep = value;
		}

		public function get cell_deep():Number
		{
			return _cell_deep;
		}

		public function set cell_deep(value:Number):void
		{
			_cell_deep = value;
		}

		public function get padding():Number
		{
			return _padding;
		}

		public function set padding(value:Number):void
		{
			_padding = value;
		}

		public function get border():Number
		{
			return _border;
		}

		public function set border(value:Number):void
		{
			_border = value;
		}

		public function get col_count():Number
		{
			return _col_count;
		}

		public function set col_count(value:Number):void
		{
			_col_count = value;
		}

		public function get row_count():Number
		{
			return _row_count;
		}

		public function set row_count(value:Number):void
		{
			_row_count = value;
		}

		public function get iconName():String
		{
			return _iconName;
		}

		public function set iconName(value:String):void
		{
			_iconName = value;
		}

		public function get card_image():String
		{
			return _card_image;
		}

		public function set card_image(value:String):void
		{
			_card_image = value;
		}

		public function get height():Number
		{
			return _height;
		}

		public function set height(value:Number):void
		{
			_height = value;
		}

		public function get width():Number
		{
			return _width;
		}

		public function set width(value:Number):void
		{
			_width = value;
		}

		public function get imageName():String{
			return _imageName;
		}
		
		public function set imageName(imageName:String):void{
			this._imageName=imageName;
		}
		
		public function get label():String{
			return this._label;
		}
		
		public function set label(label:String):void{
			this._label=label;
		}
		
		public function get shapeType():String{
			return _shapeType;
		}
		
		public function set shapeType(shapeType:String):void{
			this._shapeType=shapeType;
		}
		
		public var action:Function;

		public function get labelxoff():Number
		{
			return _labelxoff;
		}

		public function set labelxoff(value:Number):void
		{
			_labelxoff = value;
		}

		public function get labelyoff():Number
		{
			return _labelyoff;
		}

		public function set labelyoff(value:Number):void
		{
			_labelyoff = value;
		}

		public function get border_top():Number
		{
			return _border_top;
		}

		public function set border_top(value:Number):void
		{
			_border_top = value;
		}


	}
}