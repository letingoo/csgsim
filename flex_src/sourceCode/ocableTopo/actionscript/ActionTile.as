package   sourceCode.ocableTopo.actionscript
{
	public class ActionTile
	{
		private var _imageName:String;
		private var _label:String;
		private var _shapeType:String;
		private var _imageSizeforTitle:Number;
		private var _imageSizeforNode:Number;
		private var _type:String;
		private var _linkColor:String;
		
		public function get type():String{
			return _type;
		}
		
		public function set type(type:String):void{
			this._type=type;
		}
		
		public function get linkColor():String{
			return _linkColor;
		}
		
		public function set linkColor(linkColor:String):void{
			this._linkColor=linkColor;
		}
		
		
		
		public function get imageName():String{
			return _imageName;
		}
		
		public function set imageName(imageName:String):void{
			this._imageName=imageName;
		}
		
		public function get label():String{
			return this._label;
		}
		
		public function set label(label:String):void{
			this._label=label;
		}
		
		public function get shapeType():String{
			return _shapeType;
		}
		
		public function set shapeType(shapeType:String):void{
			this._shapeType=shapeType;
		}
		
		public function get imageSizeforTitle():Number{
			return _imageSizeforTitle;
		}
		
		public function set imageSizeforTitle(imageSizeforTitle:Number):void{
			this._imageSizeforTitle=imageSizeforTitle;
		}
		
		public function get imageSizeforNode():Number{
			return _imageSizeforNode;
		}
		
		public function set imageSizeforNode(imageSizeforNode:Number):void{
			this._imageSizeforNode=imageSizeforNode;
		}
		
		public var action:Function;
	}
}