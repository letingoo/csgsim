<?xml version="1.0" encoding="utf-8"?>  
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml"  
				layout="absolute" 
				width="620" 
				height="370" 
				backgroundImage="@Embed(source='assets/images/popwindowbg.png')"
				backgroundSize="100%"
				title=""
				showCloseButton="true"
				close="close()"
				fontFamily="宋体" fontSize="12"
				creationComplete="init()"> 
	<mx:Style>
		.HBox{
			horizontalAlign:center;
			verticalAlign:bottom;
		}
		.HBoxButton{
			horizontalAlign:center;
			verticalAlign:middle;
			backgroundSize:'100%';
			backgroundImage:Embed(source='assets/images/popwindowbtnbg.png');
		}
		.searchhbox{
			paddingLeft:15;
			paddingRight:15;
		}
	</mx:Style>
	<mx:Script source="../actionscript/ShowSearchView.as" />  
	<mx:VBox width="100%" 
			 height="100%" 
			 paddingTop="0" 
			 paddingBottom="0" 
			 paddingLeft="0" 
			 paddingRight="0" 
			 horizontalAlign="center" id="property" fontFamily="宋体" fontSize="12">
		<mx:HBox width="100%" id="propertychild" height="82%">
		</mx:HBox>
	</mx:VBox>
</mx:TitleWindow>  
