package sourceCode.equipGraph.model
{
	[Bindable]	
	[RemoteClass(alias="devicepanel.model.PackInfoModel")] 
	public class PackInfoModel
	{
		public var equipname:String;//设备名称
		public var frameserial:String;//机框序号
		public var slotserial:String;//机槽型号
		public var packmodel:String;//机盘型号
		public var packserial:String;//机盘序号
		public var updatedate:String;//更新时间
		public var remark:String;//备注
		public var updateperson:String;//更新人
		public var packsn:String;//序列号
		public var software_version:String;// 软件版本
		public var hardware_version:String;//硬件版本
		public function EquipInfo()
		{
			equipname = "";//设备名称
			frameserial = "";//机框序号
			slotserial = "";//机槽型号
			packmodel = "";//机盘型号
			packserial = "";//机盘序号
			updatedate = "";//更新时间
			remark = "";//备注
			updateperson = "";//更新人
			packsn="";
			software_version="";
			hardware_version="";
		}
	}
}