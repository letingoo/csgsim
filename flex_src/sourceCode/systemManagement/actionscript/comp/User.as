	// ActionScript file
	import mx.controls.Alert;
	[Bindable]
	private var _usename:String;
	
	[Bindable]
	private var _ip:String;
    
    [Bindable]
	private var _time:String;
	
	public function get time():String
	{
		return _time;
	}
	public function set time(value:String):void
	{
		_time=value;
	}
	public function get ip():String
	{
		return _ip;
	}
	
	public function set ip(value:String):void
	{
		_ip = value;
	}
	
	public function get usename():String
	{
		return _usename;
	}
	
	public function set usename(value:String):void
	{
		_usename = value;
	}
	
	
	protected function rollOverHandler(event:MouseEvent):void
	{
		this.setStyle("backgroundColor","#CCDDEE");
	}
	
	
	protected function rollOutHandler(event:MouseEvent):void
	{
		this.setStyle("backgroundColor","");
	}