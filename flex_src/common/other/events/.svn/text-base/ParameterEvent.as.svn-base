package common.other.events
{
	import flash.events.Event;
	
	import sourceCode.tableResurces.model.BindableObject;

	public class ParameterEvent extends Event
	{
		[Bindable]
		public var parameter1:Object;
		public var parameter2:Object;
		public var parameter3:Object;
		public var parameter4:Object;
		public function ParameterEvent(type:String,parameter1:Object=null,parameter2:Object=null,parameter3:Object=null,parameter4:Object=null)
		{
			super(type);
			this.parameter1=parameter1;
			this.parameter2=parameter2;
			this.parameter3=parameter3;
			this.parameter4=parameter4;
		}
	}
}