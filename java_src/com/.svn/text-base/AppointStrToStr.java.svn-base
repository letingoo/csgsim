public   class AppointStrToStr {
     public  String judgeStrIsNull(String str,String judgeStr,String returnStr){
        if(judgeStr==null){
          if (str == judgeStr) {
            str = returnStr;
          }   
        } else{
          if (str.equalsIgnoreCase(judgeStr)) {
            str = returnStr;
          }
        }
        return str;
    }
}
